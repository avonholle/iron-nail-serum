  title = {Serum {{Urate}}, {{Genetic Variation}}, and {{Prostate Cancer Risk}}: Atherosclerosis {{Risk}} in {{Communities}} ({{ARIC}}) {{Study}}},
  shorttitle = {Serum {{Urate}}, {{Genetic Variation}}, and {{Prostate Cancer Risk}}},
  author = {Wang, Anqi and Barber, John R. and Tin, Adrienne and Marzo, Angelo M. De and Kottgen, Anna and Joshu, Corinne E. and Platz, Elizabeth A.},
  date = {2019-07-01},
  journaltitle = {Cancer Epidemiology and Prevention Biomarkers},
