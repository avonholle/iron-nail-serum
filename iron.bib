  author = {Zacharias, Helena U. and Rehberg, Thorsten and Mehrl, Sebastian and Richtmann, Daniel and Wettig, Tilo and Oefner, Peter J. and Spang, Rainer and Gronwald, Wolfram and Altenbuchinger, Michael},
  date = {2017-10-06},
  journaltitle = {Journal of Proteome Research},
  shortjournal = {J. Proteome Res.},
