@article{rashaidHeavyMetalsTrace2021,
  title = {Heavy Metals and Trace Elements in Scalp Hair Samples of Children with Severe Autism Spectrum Disorder: A Case-Control Study on {{Jordanian}} Children},
  shorttitle = {Heavy Metals and Trace Elements in Scalp Hair Samples of Children with Severe Autism Spectrum Disorder},
  author = {Rashaid, Ayat Hussein B. and Nusair, Shreen Deeb and Alqhazo, Mazin Taha and Adams, James B. and Abu-Dalo, Muna Ahmed and Bashtawi, Mahmoud Ahmad},
