  author = {Chessa, Marco A. and Iorizzo, Matilde and Richert, Bertrand and López-Estebaranz, Jose L. and Rigopoulos, Dimitrios and Tosti, Antonella and Gupta, Aditya K. and Di Chiacchio, Nilton and Di Chiacchio, Nilton G. and Rubin, Adam I. and Baran, Robert and Lipner, Shari R. and Daniel, Ralph and Chiheb, Soumya and Grover, Chander and Starace, Michela and Piraccini, Bianca M.},
