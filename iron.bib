  author = {Li, Pengcheng and Zheng, Xun and Shou, Kangquan and Niu, Yahui and Jian, Chao and Zhao, Yong and Yi, Wanrong and Hu, Xiang and Yu, Aixi},
  date = {2016-12-15},
  journaltitle = {American Journal of Translational Research},
