@article{ISI:000409118200029,
  title = {Estimation of Serum Ferritin Level in Potentially Malignant Disorders, Oral Squamous Cell Carcinoma, and Treated Cases of Oral Squamous Cell Carcinoma},
  author = {Khanna, Vikram and Karjodkar, Freny and Robbins, Snigdha and Behl, Manisha and Arya, Saurabh and Tripathi, Anurag},
  date = {2017-07/2017-09},
  journaltitle = {JOURNAL OF CANCER RESEARCH AND THERAPEUTICS},
  volume = {13},
  number = {3},
  pages = {\{550-555\}},
  issn = {0973-1482},
  doi = {\{10.4103/0973-1482.181182\}},
  abstract = {Introduction: Serum ferritin is the best indicator of iron stores in the body. The level of serum ferritin was found to be elevated in number of malignancies. Serum ferritin is also been established as prognostic marker for malignant lymphoma, cervical cancer, breast cancer, etc. Aims and Objective: The aim of this study is to evaluate the level of serum ferritin in patients with pretreatment and posttreatment of squamous cell carcinoma and to determine the significance of serum ferritin as prognostic marker for oral squamous cell carcinoma (OSCC). Materials and Methods: Blood samples of 15 normal, thirty potentially malignant lesions, and thirty patients of squamous cell carcinoma were taken and analyzed for serum ferritin using ELISA. Of thirty patients, 15 patients were followed up till 6 months after their treatment and serum ferritin was again determined. Results: The average serum ferritin of 30 potentially malignant disorders was 62.70 ng/ml, and thirty patients with squamous cell carcinoma were found to be 162.47 ng/dl. After 6 months of the treatment, the mean fall in serum ferritin with nonrecurrence patients was 84 ng/dl and increase in the serum ferritin in recurrence cases was 13.4 ng/dl. Conclusion: Determining serum ferritin is a convenient and nonexpensive method to determine the outcome of the treatment of the cases with OSCC. Its potential as prognostic marker could not be overlooked.},
  eissn = {1998-4138},
  keywords-plus = {IRON STORES; CANCER; LESIONS; NECK; HEAD},
  times-cited = {3},
  unique-id = {ISI:000409118200029},
  keywords = {(Oral squamous cell carcinoma,potentially malignant disorders,serum  ferritin)}
}

@article{ISI:000411169800009,
  title = {Haloperidol, a Sigma Receptor 1 Antagonist, Promotes Ferroptosis in Hepatocellular Carcinoma Cells},
  author = {Bai, Tao and Wang, Shuai and Zhao, Yipu and Zhu, Rongtao and Wang, Weijie and Sun, Yuling},
  date = {2017-09-30},
  journaltitle = {BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS},
  volume = {491},
  number = {4},
  pages = {\{919-925\}},
  issn = {0006-291X},
  doi = {\{10.1016/j.bbrc.2017.07.136\}},
  abstract = {Ferroptosis is a novel form of cell death, which is characterized by accumulation of reactive oxygen species (ROS). Sigma 1 receptor (SIR) has been suggested to function in oxidative stress metabolism. Both erastin and sorafenib significantly induced S1R protein expression. Haloperidol strongly promoted erastin- and sorafenib-induced cell death, which was blocked by ferrostatin-1 but not ZVAD-FMK or necrosulfonamide. During ferroptosis, haloperidol substantially increased the cellular levels of Fe2+, GSH and lipid peroxidation. Furthermore, several ferroptosis-related protein targets were up-regulated in the absence of haloperidol. Thus, Our study identified an association between haloperidol and ferroptosis for the first time. Our analyses of a combination of drugs may provide a novel strategy of hepatocellular carcinoma (HCC) therapy. (C) 2017 Elsevier Inc. All rights reserved.},
  eissn = {1090-2104},
  keywords-plus = {TARGETED THERAPIES; OXIDATIVE STRESS; IN-VITRO; SORAFENIB; DEATH; PROTECTS; CANCER; TUMOR; FORM; ACTIVATION},
  times-cited = {5},
  unique-id = {ISI:000411169800009},
  keywords = {(Ferroptosis,Haloperidol,Hepatocellular carcinoma,Sorafenib)}
}

@article{ISI:000412346100007,
  title = {Ferroptosis: A {{Regulated Cell Death Nexus Linking Metabolism}}, {{Redox Biology}}, and {{Disease}}},
  author = {Stockwell, Brent R. and Angeli, Jose Pedro Friedmann and Bayir, Hulya and Bush, Ashley I. and Conrad, Marcus and Dixon, Scott J. and Fulda, Simone and Gascon, Sergio and Hatzios, Stavroula K. and Kagan, Valerian E. and Noel, Kay and Jiang, Xuejun and Linkermann, Andreas and Murphy, Maureen E. and Overholtzer, Michael and Oyagi, Atsushi and Pagnussat, Gabriela C. and Park, Jason and Ran, Qitao and Rosenfeld, Craig S. and Salnikow, Konstantin and Tang, Daolin and Torti, Frank M. and Torti, Suzy V. and Toyokuni, Shinya and Woerpel, K. A. and Zhang, Donna D.},
  date = {2017-10-05},
  journaltitle = {CELL},
  volume = {171},
  number = {2},
  pages = {\{273-285\}},
  issn = {0092-8674},
  doi = {\{10.1016/j.cell.2017.09.021\}},
  abstract = {Ferroptosis is a form of regulated cell death characterized by the iron-dependent accumulation of lipid hydroperoxides to lethal levels. Emerging evidence suggests that ferroptosis represents an ancient vulnerability caused by the incorporation of polyunsaturated fatty acids into cellular membranes, and cells have developed complex systems that exploit and defend against this vulnerability in different contexts. The sensitivity to ferroptosis is tightly linked to numerous biological processes, including amino acid, iron, and polyunsaturated fatty acid metabolism, and the biosynthesis of glutathione, phospholipids, NADPH, and coenzyme Q(10). Ferroptosis has been implicated in the pathological cell death associated with degenerative diseases (i.e., Alzheimer's, Huntington's, and Parkinson's diseases), carcinogenesis, stroke, intracerebral hemorrhage, traumatic brain injury, ischemia-reperfusion injury, and kidney degeneration in mammals and is also implicated in heat stress in plants. Ferroptosis may also have a tumor-suppressor function that could be harnessed for cancer therapy. This Primer reviews the mechanisms underlying ferroptosis, highlights connections to other areas of biology and medicine, and recommends tools and guidelines for studying this emerging form of regulated cell death.},
  eissn = {1097-4172},
  keywords-plus = {MILD COGNITIVE IMPAIRMENT; GLUTATHIONE-PEROXIDASE 4; ALZHEIMERS-DISEASE; PARKINSONS-DISEASE; LIPID-PEROXIDATION; OXIDATIVE STRESS; CANCER-CELLS; IN-VIVO; VITAMIN-E; IRON},
  orcid-numbers = {Conrad, Marcus/0000-0003-1140-5612 Toyokuni, Shinya/0000-0002-5757-1109 Fulda, Simone/0000-0002-0459-6417 Bush, Ashley/0000-0001-8259-9069},
  researcherid-numbers = {Conrad, Marcus/E-8792-2015 Toyokuni, Shinya/C-1358-2010},
  times-cited = {102},
  unique-id = {ISI:000412346100007}
}

