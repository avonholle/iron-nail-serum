@article{ISI:000265970400001,
  title = {Cytochrome c/Cardiolipin Relations in Mitochondria: A Kiss of Death},
  author = {Kagan, Valerian E. and Bayir, Huelya A. and Belikova, Natalia A. and Kapralov, Olexandr and Tyurina, Yulia Y. and Tyurin, Vladimir A. and Jiang, Jianfei and Stoyanovsky, Detcho A. and Wipf, Peter and Kochanek, Patrick M. and Greenberger, Joel S. and Pitt, Bruce and Shvedova, Anna A. and Borisenko, Grigory},
  date = {2009-06-01},
  journaltitle = {FREE RADICAL BIOLOGY AND MEDICINE},
  volume = {46},
  number = {11},
  pages = {\{1439-1453\}},
  issn = {0891-5849},
  doi = {\{10.1016/j.freeradbiomed.2009.03.004\}},
  abstract = {Recently, phospholipid peroxidation products gained a reputation as key regulatory molecules and participants in oxidative signaling pathways. During apoptosis, a mitochondria-specific phospholipid, cardiolipin (CL), interacts with cytochrome c (cyt c) to form a peroxidase complex that catalyzes CL oxidation; this process plays a pivotal role in the mitochondrial stage of the execution of the cell death program. This review is focused on redox mechanisms and essential structural features of cyt c's conversion into a CL-specific peroxidase that represent an interesting and maybe still unique example of a functionally significant ligand change in hemoproteins. Furthermore, specific characteristics of CL in mitochondria-its asymmetric transmembrane distribution and mechanisms of collapse, the regulation of its synthesis, remodeling, and fatty acid composition-are given significant consideration. Finally, new concepts in drug discovery based on the design of mitochondria-targeted inhibitors of cyt c/CL peroxidase and CL peroxidation with antiapoptotic effects are presented. Published by Elsevier Inc.},
  keywords-plus = {LINOLEIC-ACID HYDROPEROXIDE; POLYUNSATURATED FATTY-ACIDS; SITE-DIRECTED MUTAGENESIS; ELECTRON-TRANSPORT CHAIN; EXTENDED LIPID ANCHORAGE; PHOSPHORYLATED IN-VIVO; RAT-LIVER MITOCHONDRIA; PROGRAMMED CELL-DEATH; HYDROGEN-PEROXIDE; OXIDATIVE LIPIDOMICS},
  orcid-numbers = {Kochanek, Patrick/0000-0002-2627-913X Tyurin, Vladimir/0000-0002-3474-1697 Kapralov, Oleksandr/0000-0002-3006-916X Stoyanovsky, Detcho/0000-0001-5591-4780},
  researcherid-numbers = {Kochanek, Patrick/D-2371-2015},
  times-cited = {244},
  unique-id = {ISI:000265970400001},
  keywords = {(Cytochrome c,Apoptosis,Autophagy,Cardiolipin,Free radicals),Lipidomics,Mitochondrial targeting,Oxidative stress,Peroxidase}
}

