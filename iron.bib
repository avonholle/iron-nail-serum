@article{reevesCalculationHaemIron2017,
  title = {Calculation of {{Haem Iron Intake}} and {{Its Role}} in the {{Development}} of {{Iron Deficiency}} in {{Young Women}} from the {{Australian Longitudinal Study}} on {{Women}}’s {{Health}}},
  author = {Reeves, Angela J. and McEvoy, Mark A. and MacDonald-Wicks, Lesley K. and Barker, Daniel and Attia, John and Hodge, Allison M. and Patterson, Amanda J.},
  date = {2017-05},
  journaltitle = {Nutrients},
