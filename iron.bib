@article{vanwyheEffectStatinsBreast2017,
  title = {Effect of Statins on Breast Cancer Recurrence and Mortality: A Review},
  shorttitle = {Effect of Statins on Breast Cancer Recurrence and Mortality},
  author = {Van Wyhe, Renae D and Rahal, Omar M and Woodward, Wendy A},
  date = {2017-12-01},
  journaltitle = {Breast Cancer : Targets and Therapy},
  shortjournal = {Breast Cancer (Dove Med Press)},
  volume = {9},
