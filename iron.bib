@article{laboratoryTEACHINGYOUWAYS,
  title = {Latent Class Models for Joint Analysis of Disease Prevalence and High-Dimensional Semicontinuous Biomarker Data},
  author = {Zhang, Bo and Chen, Zhen and Albert, Paul S.},
