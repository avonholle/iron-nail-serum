@article{ISI:A1992HJ90500009,
  title = {{{RIBONUCLEOTIDE REDUCTASE}} - {{REGULATION}}, {{REGULATION}}, {{REGULATION}}},
  author = {ELLEDGE, SJ and ZHENG, Z and ALLEN, JB},
