@article{linnetNecessarySampleSize1999,
  title = {Necessary {{Sample Size}} for {{Method Comparison Studies Based}} on {{Regression Analysis}}},
  author = {Linnet, Kristian},
  date = {1999-06-01},
  journaltitle = {Clinical Chemistry},
  volume = {45},
