@article{loAssociationMeatConsumptiona,
  title = {Association between Meat Consumption and Risk of Breast Cancer: Findings from the {{Sister Study}}},
  shorttitle = {Association between Meat Consumption and Risk of Breast Cancer},
  author = {Lo, Jamie J. and Park, Yong-Moon Mark and Sinha, Rashmi and Sandler, Dale P.},
