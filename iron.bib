@article{mascitelliAspirinassociatedIronLoss2010,
  title = {Aspirin-Associated Iron Loss as an Anticancer Mechanism},
  author = {Mascitelli, Luca and Pezzetta, Francesca and Sullivan, Jerome L.},
  date = {2010-01-01},
  journaltitle = {Medical Hypotheses},
