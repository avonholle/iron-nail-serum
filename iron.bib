@article{dormuthLIKELIHOODRATIOMETAANALYSIS2016,
  title = {{{LIKELIHOOD RATIO META}}-{{ANALYSIS}}: {{NEW MOTIVATION AND APPROACH FOR AN OLD METHOD}}},
