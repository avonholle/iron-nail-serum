@article{ISI:000302013200019,
  title = {Dietary Intake of Heme Iron and Risk of Gastric Cancer in the {{European}} Prospective Investigation into Cancer and Nutrition Study},
  author = {Jakszyn, Paula and Agudo, Antonio and Lujan-Barroso, Leila and Bueno-de-Mesquita, H. Bas and Jenab, Mazda and Navarro, Carmen and Palli, Domenico and Boeing, Heiner and Manjer, Jonas and Numans, Mattijs E. and Igali, Laszlo and Boutron-Ruault, Marie-Christine and Clavel-Chapelon, Francoise and Morois, Sophie and Grioni, Sara and Panico, CSalvatore and Tumino, Rosario and Sacerdote, Carlotta and Ramon Quiros, J. and Molina-Montes, Esther and Huerta Castano, Jose Ma and Barricarte, Aurelio and Amiano, Pilar and Khaw, Kay-Tee and Wareham, Nicholas and Allen, Naomi E. and Key, Timothy J. and Jeurnink, Suzanne M. and Peeters, Petra H. M. and Bamia, Christina and Valanou, Elisabeth and Trichopoulou, Antonia and Kaaks, Rudolf and Lukanova, Annekatrin and Bergmann, Manuela M. and Lindkvist, Bjorn and Stenling, Roger and Johansson, Ingegerd and Dahm, Christina C. and Overvad, Kim and Olsen, Anja and Tjonneland, Anne and Skeie, Guri and Ragnhild Broderstad, Ann and Lund, Eiliv and Michaud, Dominique S. and Mouw, Traci and Riboli, Elio and Gonzalez, Carlos A.},
  date = {2012-06-01},
  journaltitle = {INTERNATIONAL JOURNAL OF CANCER},
  volume = {130},
  number = {11},
  pages = {\{2654-2663\}},
  issn = {0020-7136},
  doi = {\{10.1002/ijc.26263\}},
  abstract = {Even though recent studies suggest that a high intake of heme iron is associated with several types of cancer, epidemiological studies in relation to gastric cancer (GC) are lacking. Our previous results show a positive association between red and processed meat and non cardia gastric cancer, especially in Helicobacter pylori infected subjects. The aim of the study is to investigate the association between heme iron intake and GC risk in the European prospective investigation into cancer and nutrition (EURGAST-EPIC). Dietary intake was assessed by validated center-specific questionnaires. Heme iron was calculated as a type-specific percentage of the total iron content in meat intake, derived from the literature. Antibodies of H. pylori infection and vitamin C levels were measured in a sub-sample of cases and matched controls included in a nested case-control study within the cohort. The study included 481,419 individuals and 444 incident cases of GC that occurred during an average of 8.7 years of followup. We observed a statistically significant association between heme iron intake and GC risk (HR 1.13 95\% CI: 1.011.26 for a doubling of intake) adjusted by sex, age, BMI, education level, tobacco smoking and energy intake. The positive association between heme iron and the risk of GC was statistically significant in subjects with plasma vitamin C {$<$}39 mmol/l only (log2 HR 1.54 95\% CI (1.012.35). We found a positive association between heme iron intake and gastric cancer risk.},
  keywords-plus = {N-NITROSO COMPOUNDS; HELICOBACTER-PYLORI INFECTION; IOWA WOMENS HEALTH; EPIC-EURGAST; RED MEAT; ENDOGENOUS FORMATION; BLADDER-CANCER; PROCESSED MEAT; STOMACH-CANCER; FREE-RADICALS},
  orcid-numbers = {Boutron-Ruault, Marie-Christine/0000-0002-5956-5693 Gonzalez, Carlos A/0000-0003-2822-9715 Grioni, Sara/0000-0002-5891-8426 Agudo, Antonio/0000-0001-9900-5677 Dahm, Christina/0000-0003-0481-2893 Jakszyn, Paula/0000-0003-0672-8847 Riboli, Elio/0000-0001-6795-6080 Lindkvist, Bjorn/0000-0001-7485-0006 Molina-Montes, Esther/0000-0002-0428-2426 Navarro, Carmen/0000-0001-8896-7483 Tjonneland, Anne/0000-0003-4385-2097 Valanou, Elissavet Maria/0000-0003-1970-6460 PALLI, Domenico/0000-0002-5558-2437 Skeie, Guri/0000-0003-2476-4251 Lujan-Barroso, Leila/0000-0001-6224-1764 Olsen, Anja/0000-0003-4788-503X Sacerdote, Carlotta/0000-0002-8008-5096 Huerta, Jose Maria/0000-0002-9637-3869},
  researcherid-numbers = {Boutron-Ruault, Marie-Christine/H-3936-2014 Gonzalez, Carlos A/O-4651-2014 Grioni, Sara/K-5320-2016 Michaud, Dominique/I-5231-2014 Agudo, Antonio/J-1805-2016 Dahm, Christina/G-9787-2014 Jakszyn, Paula/K-1458-2016 Clavel-Chapelon, Francoise/G-6733-2014},
