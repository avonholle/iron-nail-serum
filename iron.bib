  author = {Sørensen, Erik and Rigas, Andreas S. and Thørner, Lise W. and Burgdorf, Kristoffer S. and Pedersen, Ole B. and Petersen, Mikkel S. and Hjalgrim, Henrik and Erikstrup, Christian and Ullum, Henrik},
