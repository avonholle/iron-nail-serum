@article{kehmRegularUseAspirin2019,
  title = {Regular Use of Aspirin and Other Non-Steroidal Anti-Inflammatory Drugs and Breast Cancer Risk for Women at Familial or Genetic Risk: A Cohort Study},
  shorttitle = {Regular Use of Aspirin and Other Non-Steroidal Anti-Inflammatory Drugs and Breast Cancer Risk for Women at Familial or Genetic Risk},
  author = {Kehm, Rebecca D. and Hopper, John L. and John, Esther M. and Phillips, Kelly-Anne and MacInnis, Robert J. and Dite, Gillian S. and Milne, Roger L. and Liao, Yuyan and Zeinomar, Nur and Knight, Julia A. and Southey, Melissa C. and Vahdat, Linda and Kornhauser, Naomi and Cigler, Tessa and Chung, Wendy K. and Giles, Graham G. and McLachlan, Sue-Anne and Friedlander, Michael L. and Weideman, Prue C. and Glendon, Gord and Nesci, Stephanie and Andrulis, Irene L. and Buys, Saundra S. and Daly, Mary B. and Terry, Mary Beth},
