  author = {VelezPardo, C and DelRio, MJ and Verschueren, H and Ebinger, G and Vauquelin, G},
  date = {1997-02},
  journaltitle = {PHARMACOLOGY \& TOXICOLOGY},
  volume = {80},
  number = {2},
  pages = {\{76-84\}},
  issn = {0901-9928},
  doi = {\{10.1111/j.1600-0773.1997.tb00287.x\}},
  abstract = {\{Recent studies have shown that Fe2+ increases the oxidation of  monoamines such as serotonin, dopamine and related toxins and that the  formed oxidation products call undergo co-valent binding to free  sulphydryl groups of proteins such as actin and `'serotonin binding  proteins'' which are present in soluble brain extracts. Here we have  tested the ability of ferrous iron to induce \{[\}H-3]dopamine association  to cytoplasmic proteins and we have established that a similar oxidation  mechanism evidenced iii vitro a studies could be applied in cell  culture. When PC12 cells were incubated with Ferrous iron (ferrocene).  the binding of \{[\}H-3]dopamine to proteins was found to be two fold  increased with respect to control. The iron is likely to accelerate the  oxidation of dopamine to produce quinones which covalently bind to  proteins and induce high-molecular protein aggregates. We evidenced that  dopamine/from combination induced cell death in undifferentiated PC12  cells via an active cellular process evaluated in terms of morphological  and biochemical changes indicative of apoptosis. We also demonstrated  induction of lipid peroxidation when dopamine and ferrocene were present  in high concentrations. Moreover, ascorbic acid diminished apoptosis but  not the lipid peroxidation process. It might indicate that ferrocene and  dopamine could produce oxidative stress of a different nature. These  results show that the actions of dopamine and iron are essential in the  induction of apoptosis and lipid peroxidation. However. there is no  necessary causual link between lipid peroxidation and apoptosis. Our  data also suggest that iron is capable of increasing the cytotoxicity of  dopamine merely by increasing its rate of oxidation and without  intervention of the monoamine oxidase B enzyme and, hence, both  phenomenons may occur independently from each other ill rat  pheochromocytoma PC12. These observations may have relevance to the  understanding of the mechanism by which dopaminergic neurones are  destroyed in some neurodegenerative disorders.\}},
  keywords-plus = {NERVE GROWTH-FACTOR; SEROTONIN BINDING-PROTEINS; PARKINSONS-DISEASE; SUBSTANTIA-NIGRA; NEURODEGENERATIVE DISEASES; BUTHIONINE SULFOXIMINE; GLUTATHIONE METABOLISM; SELECTIVE MODIFICATION; SYMPATHETIC NEURONS; LIPID-PEROXIDATION},
  researcherid-numbers = {Verschueren, Hendrik/B-9301-2009},
  times-cited = {64},
  unique-id = {ISI:A1997WH92400004}
}

@article{ISI:A1997XM38300003,
  title = {A Phase {{II}} Trial of Deferoxamine in Patients with Hormone-Refractory Metastatic Prostate Cancer},
  author = {Dreicer, R and Kemp, JD and Stegink, LD and Cardillo, T and Davis, CS and Forest, PK and See, WA},
