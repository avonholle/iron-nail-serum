@article{lynchBiomarkersNutritionDevelopment2018,
  title = {Biomarkers of {{Nutrition}} for {{Development}} ({{BOND}})-{{Iron Review}}},
  author = {Lynch, Sean and Pfeiffer, Christine M. and Georgieff, Michael K. and Brittenham, Gary and Fairweather-Tait, Susan and Hurrell, Richard F. and McArdle, Harry J. and Raiten, Daniel J.},
  date = {2018-06-01},
  journaltitle = {The Journal of Nutrition},
