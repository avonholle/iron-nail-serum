  keywords = {alpha-tocopherol,antioxidant,cardiac ischemia,Cardiovascular,heart,inflammation,Inflammation,Markers,mitochondria,nf-kappa-b,nitric-oxide synthase,Oxidation,superoxide-dismutase,therapeutic   target},
