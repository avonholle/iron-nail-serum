  author = {Welsh, J. L. and Wagner, B. A. and van't Erve, T. J. and Zehr, P. S. and Berg, D. J. and Halfdanarson, T. R. and Yee, N. S. and Bodeker, K. L. and Du, J. and Roberts, II, L. J. and Drisko, J. and Levine, M. and Buettner, G. R. and Cullen, J. J.},
  options = {useprefix=true},
