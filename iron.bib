@article{liuNmycDownstreamRegulated2012,
  title = {N-Myc Downstream Regulated Gene 1 Modulates {{Wnt}}-β-Catenin Signalling and Pleiotropically Suppresses Metastasis},
  author = {Liu, Wen and Xing, Fei and Iiizumi-Gairani, Megumi and Okuda, Hiroshi and Watabe, Misako and Pai, Sudha K. and Pandey, Puspa R. and Hirota, Shigeru and Kobayashi, Aya and Mo, Yin-Yuan and Fukuda, Koji and Li, Yi and Watabe, Kounosuke},
  date = {2012-02-01},
