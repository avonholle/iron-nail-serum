@article{milmanIronStatusMarkers,
  title = {Iron Status Markers, Serum Ferritin and Hemoglobin in 1359 {{Danish}} Women in Relation to Menstruation, Hormonal Contraception, Parity, and Postmenopausal Hormone Treatment},
  author = {Milman, N and Kirchhoff, M and Jorgensen, T},
  pages = {7},
  abstract = {Iron status was assessed by measuring serum (S-) ferritin and hemoglobin (Hb) in a population survey comprising 1359 nonpregnant Danish women, in age cohorts of 30, 40, 50, and 60 years; 809 were premenopausal and 550 postmenopausal. Median age for menarche was 14 years, for menopause (artificial and natural) 48 years. Premenopausal women had lower S-ferritin (median 37/\textasciitilde g/1) than postmenopausal women (median 71/\textasciitilde g/1; p {$<$} 0.0001). Of the premenopausal women, 17.7\textasciitilde{} had S-ferritin {$<$} 15/tg/1 (i.e., depleted iron stores), and 23.1\textasciitilde{} S-ferritin of 1 5 - 3 0 / t g / l (i.e., small iron stores). Corresponding figures in postmenopausal women were 3.3\% and 10.3\textasciitilde{} Hb values in premenopausal women were mean 137 + 10 (SD) g/1 (8.5 + 0.6 mmol/1) vs. 140 + 10 g/l (8.7 + 0.6 mmol/l) in postmenopausal women (19 {$<$} 0.0001), 4.1070 of pre- and 3.3070 of postmenopausal women had values {$<$} 121 g/l (7.5 mmol/1). Iron deficiency anemia (i.e., S-ferritin {$<$} 15/tg/1 and Hb {$<$} 121 g/l) was found in 2.6070 of pre- and 0.36070 of postmenopausal women. Premenopausal multipara had lower S-ferritin than nulli- and unipara (p {$<$} 0.04). The use of oral contraceptives had a marked influence on iron stores; premenopausal women taking the pill had higher S-ferritin and a lower frequency of depleted iron reserves than nonusers (/7 {$<$}0.01). Postmenopausal estrogen treatment had no influence on S-ferritin or Hb.},
  langid = {english},
  file = {C\:\\Users\\vonholleaf\\Zotero\\storage\\IPTI2L95\\Milman et al. - Iron status markers, serum ferritin and hemoglobin.pdf}
}

