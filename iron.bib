@article{schnetzDisturbedIronPhenotype2020,
  title = {The {{Disturbed Iron Phenotype}} of {{Tumor Cells}} and {{Macrophages}} in {{Renal Cell Carcinoma Influences Tumor Growth}}},
  author = {Schnetz, Matthias and Meier, Julia K. and Rehwald, Claudia and Mertens, Christina and Urbschat, Anja and Tomat, Elisa and Akam, Eman A. and Baer, Patrick and Roos, Frederik C. and Brüne, Bernhard and Jung, Michaela},
  date = {2020-03},
  journaltitle = {Cancers},
