  author = {Vari, Istvan S. and Balkau, Beverley and Kettaneh, Adrian and Andre, Philippe and Tichet, Jean and Fumeron, Frederic and Caces, Emile and Marre, Michel and Grandchamp, Bernard and Ducimetiere, Pierre},
