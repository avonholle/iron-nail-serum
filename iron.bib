  keywords = {909,Cancer,cancer biology,drug resistance Word count: 11,ferroptosis,Figures: 2,iron chelator,Iron chelator,iron metabolism,metastasis,microRNA,microRNAs,Tables: 2,tumor microenvironment},
  file = {C\:\\Users\\vonholleaf\\Zotero\\storage\\IE2CFJTW\\fonc-10-00476.pdf}
}

