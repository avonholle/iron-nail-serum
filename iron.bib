  author = {Warne, Charles D and Zaloumis, Sophie G and Bertalli, Nadine A and Delatycki, Martin B and Nicoll, Amanda J and McLaren, Christine E and Hopper, John L and Giles, Graham G and Anderson, Greg J and Olynyk, John K and Powell, Lawrie W and Allen, Katrina J and Gurrin, Lyle C and {for the HealthIron Study Investigators}},
  date = {2017-04},
  journaltitle = {Journal of Gastroenterology and Hepatology},
  shortjournal = {Journal of Gastroenterology and Hepatology},
  volume = {32},
